# Server Configuration
server.port=8080

# GitHub Configuration
github.repository.url=https://github.com/ankitkrsgh1729/documented-notes
github.repository.branch=main
github.repository.local-path=./data/repo
github.repository.file-patterns=**/*.md,**/docs/*.md,**/notes/*.md
github.repository.exclude-patterns=**/node_modules/**,**/target/**

# OpenAI Configuration
openai.api.key=your-api-key-here

# Quiz System Configuration
quiz.session.questions-per-session=5
quiz.session.default-difficulty=MEDIUM

# Progress Tracking
progress.storage.path=./data/progress.json

# Logging Configuration
logging.level.com.interview.quizsystem=INFO
logging.file.name=./logs/quiz-system.log 

# Database Configuration
spring.datasource.url=jdbc:postgresql://localhost:5432/prep-instruct-db
spring.datasource.username=postgres
spring.datasource.password=prep-instruct-pass
spring.datasource.driver-class-name=org.postgresql.Driver

# JPA Configuration
spring.jpa.hibernate.ddl-auto=update
spring.jpa.show-sql=true
spring.jpa.properties.hibernate.format_sql=true
spring.jpa.properties.hibernate.dialect=org.hibernate.dialect.PostgreSQLDialect 

# JWT Configuration
jwt.secret-key=404E635266556A586E3272357538782F413F4428472B4B6250645367566B5970
jwt.expiration=86400000  # 24 hours in milliseconds 